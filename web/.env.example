# VibeX Configuration

# VibeX Backend API URL (for development)
NEXT_PUBLIC_VIBEX_API_URL=http://localhost:8000

# VibeX Backend API URL (for production)
# NEXT_PUBLIC_VIBEX_API_URL=https://your-vibex-api.railway.app

# Development settings
NODE_ENV=development

# Optional: API Key for authentication (if implemented)
# VIBEX_API_KEY=your-api-key-here

# Optional: Default team configuration path
NEXT_PUBLIC_DEFAULT_TEAM_CONFIG=examples/auto_writer/config/team.yaml

# Optional: Analytics
NEXT_PUBLIC_ANALYTICS_ID=

# Optional: Feature Flags
NEXT_PUBLIC_ENABLE_MOCK_DATA=false